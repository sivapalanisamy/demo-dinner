.category-slider .slick-prev{
        left:10px;
        bottom:-40px;
        top:inherit;
}
.category-slider .slick-next{
        right:10px;
        bottom:-40px;
        top:inherit;        
}

.category-slider .slick-prev::before,
.category-slider .slick-next::before{
    color:#000000;
}

.slider_looking img{
    max-width:100%;
}
.looking-container{
    margin-top:150px
}
.slider_looking .slider_item {
    margin:0px 10px;
}

.product-card-quantity{
    width:100%;
    float:left;
}
.product-card-quantity .qty-row{
    width:100%;
    float:left;
    margin-top:15px;
}
.product-card-quantity .qty-row label{
    float:left;
}
.product-card-quantity .qty-row .qty-selector-card{
    float:right;
}
.product-card-quantity .qty-row .qty-selector-card .quantity-form{
    display:flex;
}